<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css"	href="css/styles.css">
    <title>ACTED</title>
</head>
<body>
<!-- TOP BAR -->
<!--@import "top-bar.html"-->
<!--./TOP BAR -->
<!-- HEADER -->
<!--@import "header.html"-->
<!--./HEADER -->
<div class="wrapper">
    <div class="content">
        <main class="main">
            <!-- SECTION BANNER -->
            <section class="form-call-back">
               <div class="container">
                   <div class="form__row">
                       <div class="form__background">
                           <div class="form">
                               <form action="#" id="form" class="form__body">
                                   <h1 class="form__title">Feedback Form</h1>
                                   <div class="form__item">
                                       <label for="formName" class="form__label">Name*</label>
                                       <input type="formName" type="text" name="name" class="form__input _req">
                                   </div>
                                   <div class="form__item">
                                       <label for="formEmail" class="form__label">E-mail*</label>
                                       <input type="formEmail" type="text" name="e-mail" class="form__input _req _email">
                                   </div>
                                   <div class="form__item">
                                       <label for="formMessage" class="form__label">Message*</label>
                                       <textarea name="message" id="formMessage" class="form__input _req"></textarea>
                                   </div>
                                   <div class="form__item">
                                       <div class="checkbox">
                                           <input id="formAgfeement" checked type="checkbox" name="agreement" class="checkbox__input _req">
                                           <label for="formAgfeement" class="form__checkbox"><span>I'm agree... personal data*</span></label>
                                       </div>
                                   </div>
                                   <button type="submit" class="form__button uppercase">Send</button>
                               </form>
                           </div>
                       </div>
                   </div>
               </div>
            </section>
        </main>
    </div>
    <!-- FOOTER -->
    <!--@import "footer.html"-->
    <!-- ./FOOTER -->
</div>
<!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script> -->
<script src="js/script.js" type="text/javascript"></script>
</body>
</html>
